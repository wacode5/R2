citEntry(entry="article",
         title = "This is the title of paper",
         author = personList( as.person("Koki Tsuyuzaki")),
         year = 2016,
         journal = "hoge journal",
         doi = "***",
         url = "http://***",
         textVersion =
         paste("K Tsuyuzaki (2016):",
               "This the title of paper",
                "hoge journal" ) )
